{
  "name": "template",
  "version": "0.0.0",
  "block_types": [
    {
      "title": "Hero",
      "name": "TemplateHero",
      "icon": "image",
      "type": "object",
      "fields": [
        {
          "title": "Lead in",
          "name": "subtitle",
          "type": "string"
        },
        {
          "title": "Rubrik",
          "name": "title",
          "type": "string"
        },
        {
          "title": "L채nktext",
          "name": "link_text",
          "type": "string"
        },
        {
          "title": "L채nk",
          "name": "link",
          "type": "string",
          "format": "uri"
        },
        {
          "title": "Bild",
          "name": "image",
          "type": "string",
          "format": "file"
        },
        {
          "title": "Show lead in",
          "name": "show_subtitle",
          "type": "boolean",
          "ui:widget": "switch"
        },
        {
          "title": "Show title",
          "name": "show_title",
          "type": "boolean",
          "ui:widget": "switch"
        },
        {
          "title": "Show button",
          "name": "show_button",
          "type": "boolean",
          "ui:widget": "switch"
        },
        {
          "title": "Style",
          "name": "style",
          "type": "object",
          "fields": [
            {
              "type": "integer",
              "name": "margin_bottom",
              "title": "Push up content",
              "minimum": -100,
              "maximum": 0
            }
          ]
        }
      ]
    },
    {
      "title": "Team",
      "name": "team-template",
      "icon": "group",
      "type": "object",
      "template_engine": "nunjucks",
      "fields": [
        {
          "title": "Rubrik",
          "name": "heading",
          "type": "string"
        },
        {
          "title": "Underrubrik",
          "name": "sub_heading",
          "type": "string"
        },
        {
          "title": "Team",
          "name": "team",
          "type": "array",
          "of": [
            {
              "title": "Team member",
              "type": "object",
              "display_field": "heading",
              "fields": [
                {
                  "title": "Bild",
                  "name": "image",
                  "type": "string",
                  "format": "file"
                },
                {
                  "title": "Rubrik",
                  "name": "heading",
                  "type": "string"
                },
                {
                  "title": "Text",
                  "name": "text",
                  "type": "text"
                },
                {
                  "title": "Social links",
                  "name": "social_links",
                  "type": "array",
                  "default": [
                    {
                      "title": "Twitter",
                      "icon": "twitter",
                      "link": "#youraccount"
                    },
                    {
                      "title": "Facebook",
                      "icon": "facebook",
                      "link": "#youraccount"
                    },
                    {
                      "title": "Instagram",
                      "icon": "instagram",
                      "link": "#youraccount"
                    }
                  ],
                  "of": [{
                    "title": "L채nk",
                    "name": "link",
                    "type": "object",
                    "display_field": "title",
                    "fields": [
                      {
                        "title": "Titel",
                        "name": "title",
                        "type": "string",
                        "default": "Titel"
                      },
                      {
                        "title": "Ikon",
                        "name": "icon",
                        "type": "string",
                        "default": "twitter",
                        "ui:widget": "icon"
                      },
                      {
                        "title": "L채nk",
                        "name": "link",
                        "type": "string",
                        "format": "uri",
                        "default": "#youraccount"
                      }
                    ]
                  }]
                }
              ]
            }
          ]
        },
        {
          "type": "text",
          "name": "text",
          "title": "Text",
          "field": {
            "height": "120px"
          }
        },
        {
          "title": "Style",
          "name": "style",
          "type": "object",
          "fields": [
            {
              "type": "integer",
              "name": "margin_bottom",
              "title": "Push up content",
              "minimum": -100,
              "maximum": 0
            }
          ]
        }
      ]
    },
    {
      "title": "Services",
      "name": "services-template",
      "icon": "file-text-o",
      "ui:css_selector": "#services",
      "template_engine": "nunjucks",
      "fields": [
        {
          "title": "Rubrik",
          "name": "heading",
          "type": "string"
        },
        {
          "title": "Underrubrik",
          "name": "sub_heading",
          "type": "string"
        },
        {
          "title": "Services",
          "name": "services",
          "type": "array",
          "display_field": "heading",
          "of": [
            {
              "title": "Service",
              "name": "service",
              "type": "object",
              "fields": [
                {
                  "title": "Icon",
                  "name": "icon",
                  "type": "string",
                  "ui:widget": "icon",
                  "default": "image"
                },
                {
                  "title": "Rubrik",
                  "type": "string",
                  "name": "heading",
                  "default": "Rubrik"
                },
                {
                  "title": "Text",
                  "name": "text",
                  "type": "text",
                  "default": "En kortare text"
                }
              ]
            }
          ]
        }      
      ]  
    },
    {
      "title": "Team",
      "name": "TemplateTeam",
      "icon": "image",
      "type": "object",
      "fields": [
        {
          "title": "Lead in",
          "name": "subtitle",
          "type": "string"
        },
        {
          "title": "Rubrik",
          "name": "title",
          "type": "string"
        }
      ]
    },
    {
      "title": "Footer",
      "name": "FooterTemplate",
      "icon": "address-card-o",
      "hidden": false,
      "global": true,
      "type": "object",
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Beskrivning",
          "name": "description",
          "type": "text"
        },
        {
          "title": "Style",
          "name": "style",
          "type": "object",
          "fields": [
            {
              "title": "title",
              "name": "title",
              "type": "string"
            },
            {
              "title": "Beskrivning",
              "name": "description",
              "type": "text"
            },
            {
              "title": "Style",
              "name": "style",
              "type": "object"
            }
          ]
        }
      ]
    }
  ],
